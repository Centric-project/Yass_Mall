    @{
        var topSearchList = shopList.Select(shop => new
        {
            id = shop.Id.ToString(),
            name = shop.Value("shopTitle"),
            url = shop.Url,
            tags = shop.HasValue("outletTags") ? shop.Value<string[]>("outletTags") : null,
            cat = "Shop"
        }).Concat(dineList.Select(dine => new
        {
            id = dine.Id.ToString(),
            name = dine.Value("shopTitle"),
            url = dine.Url,
            tags = dine.HasValue("outletTags") ? dine.Value<string[]>("outletTags") : null,
            cat = "Dine"
        }));
        topSearchList = topSearchList.OrderBy(x => x.name);
    }
    <script type="text/javascript">
        $(document).ready(function () {
            $('.navbar-toggler').click(function () {
                $('.navbar-collapse').toggle('fast')
            });
        });

        var topSearchListData = @Html.Raw(Json.Encode(topSearchList));
        //var searchGoHref = '(umbraco.library.NiceUrl(2057))';

        //Animation Script
        $(window).scroll(function () {
            AOS.init({
                easing: 'ease-out-back',
                duration: 1000
            });
        });

        $(window).on("load", function() {
            $("#loading-div").fadeOut(500);
        });
    </script>

    @RenderSection("ViewScripts", required: false)
