@inherits Umbraco.Web.Mvc.UmbracoViewPage
@{
    Layout = "Master.cshtml";

    var lang = System.Globalization.CultureInfo.CurrentCulture.TwoLetterISOLanguageName;

    ViewBag.MetaTitle = Model.Value("metaTitle");
    ViewBag.MetaDescription = Model.Value("metaDescription");
    ViewBag.MetaKeyword = Model.Value("metaKeyword");

    // set page title segment
    ViewBag.PageTitle = Model.Value("metaTitle");
}
<style type="text/css">
    div section.content-wrap {
        padding: 40px 0;
    }

    div.inner-section {
        padding: 0 0 0px;
    }
</style>
<!--Start Main Banner-->
@{
    var mrBannerImg = Model.Value<IPublishedContent>("mrMainBanner");
    if (mrBannerImg != null)
    {
        <section class="main-banner">
            <div class="container-fluid custom-container-fluid">
                <div class="main-slide">
                    <div class="slide">
                        <img src="@mrBannerImg.Url()" alt="@Model.Value("metaTitle")" class="img-fluid" />
                    </div>
                </div>
            </div>
        </section>
    }
}
<!--End Main Banner-->
<!--Start Inner Page Wrapper-->
<div class="inner-section">
    <div class="container-fluid custom-container-fluid">
        <div class="row">
            <div class="col-md-12">
                <!--Start Breadcrumbs-->
                <nav aria-label="breadcrumb" class="breadcrumb-holder">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="@Umbraco.GetDictionaryValue("logoHref")">@Umbraco.GetDictionaryValue("home")</a></li>
                        <li class="breadcrumb-seprator"><i class="fas fa-angle-right"></i></li>
                        <li class="breadcrumb-item" aria-current="page"><a href="@Umbraco.GetDictionaryValue("MallRedevelopmentHRef")">@Umbraco.GetDictionaryValue("MallRedevelopment")</a></li>
                        <li class="breadcrumb-seprator"><i class="fas fa-angle-right"></i></li>
                        <li class="breadcrumb-item active" aria-current="page">@Model.Name</li>
                    </ol>
                </nav>
                <!--End Breadcrumbs-->
                @{
                    var mrTitle = Model.Value("mrMainTitle");
                    if (mrTitle != null && !string.IsNullOrEmpty(mrTitle.ToString()))
                    {
                        <!--Start Main Title-->
                        <div class="clearfix title-holder">
                            <h1 class="title">@Model.Value("mrMainTitle")</h1>
                        </div>
                        <!--End Main Title-->
                    }
                    var mrDescrip = Model.Value<IHtmlString>("mrDescription");
                    if (mrDescrip != null && !string.IsNullOrEmpty(mrDescrip.ToString()))
                    {
                        <div class="filter-box page-content rd-smInfo">
                            <div class="row">
                                @Html.Raw(mrDescrip)
                            </div>
                        </div>
                    }
                }
