                        </div>
                    </div>
                </section>
                <br /><br />
                @{
                    if (Model.HasValue("mrStepsHTML"))
                    {
                        <!--Journey Steps-->
                        <section class="journey-box rd-steps">
                            <div class="container">
                                <div class="row">
                                    @Html.Raw(Model.Value("mrStepsHTML"))
                                </div>
                            </div>
                        </section>
                        <!--Journey Steps-->
                    }
                }
            </div>
        </div>
    </div>
    @{
        var redevExploreImage = Model.Value<IPublishedContent>("redevExploreImage");
        if (redevExploreImage != null)
        {
            <!--Start Explore Redevelopment-->
            <section class="app-box">
                <div class="container">
                    <h2><b>@Model.Value("mrExploreTitle")</b></h2>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="row">
                                <img src="@redevExploreImage.Url()" alt="@Model.Value("mrExploreTitle")" />
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="app-content">
                                @{
                                    var redevExplLogo = Model.Value<IPublishedContent>("redevExploreLogo");
                                    if (redevExplLogo != null)
                                    {
                                        <img class="etihad-logo" src="@redevExplLogo.Url()" alt="@Model.Value("mrExploreTitle")" />
                                    }
                                }
                                @Html.Raw(Model.Value("redevExploreDetails"))
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!--End Explore Redevelopment-->
        }
        IEnumerable<IPublishedContent> redevSliders = Model.Children().Where(x => x.IsVisible());
        //IEnumerable<IPublishedContent> redevSliders = Model.Content.Children().Where(x => x.IsVisible());
        if (redevSliders.Count() > 0)
        {
            <section class="inspiration">
                <div class="container">
                    <div class="col-md-12">
                        <h2><b>@Model.Value("mrExploreTitle")</b></h2>
                        <div class="inspiration-slider">
                            @foreach (IPublishedContent slideItem in redevSliders)
                            {
                                var sliderImage = slideItem.Value<IPublishedContent>("sliderImage");
                                <div><img src="@sliderImage.Url()" class="img-fluid" alt="@sliderImage.Name" /></div>
                            }
                        </div>
                    </div>
                </div>
            </section>
        }

    }
</div>
