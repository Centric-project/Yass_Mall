                    <div class="container">
                        <div class="row">
                            @{
                                if (Model.HasValue("mrSecOneDescription"))
                                {
                                    var mrSecOneVideoImg = Model.Value<IPublishedContent>("mrSecOneVideoImg");
                                    if (mrSecOneVideoImg != null)
                                    {
                                        <!--Start Section One-->
                                        <div class="col-md-6">
                                            <div class="row">
                                                @{
                                                    var obExt = mrSecOneVideoImg.Value("umbracoExtension").ToString();
                                                    if (obExt == "mp4")
                                                    {
                                                        var mrVideoPoster = Model.Value<IPublishedContent>("mrVideoPoster");
                                                        <video id="ban_video" class="tv_video hide-for-small-only" controls="controls"
                                                               poster="@mrVideoPoster.Url()">
                                                            <source src="@mrSecOneVideoImg.Url()" type="video/mp4" />
                                                            Your browser does not support the video tag.
                                                        </video>
                                                    }
                                                    else
                                                    {
                                                        <img src="@mrSecOneVideoImg.Url()" alt="" />
                                                    }
                                                }
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="cont-wrap">
                                                <div class="content-box">
                                                    @Html.Raw(Model.Value("mrSecOneDescription"))
                                                    @*<a class="know-more" href="javascript:void(0)">Know More</a>*@
