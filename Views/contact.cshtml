@helper getOpeningHours(string name, string hours)
{
    if (!string.IsNullOrWhiteSpace(Model.Value<string>(name)))
    {
        <li>
            <span class="store">@Model.Value(name)</span>
            <span class="time">@Model.Value(hours)</span>
        </li>
    }
}

