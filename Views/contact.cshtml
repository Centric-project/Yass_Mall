    var googleMapsApiKey = ConfigurationManager.AppSettings["GoogleMapsApiKey"];

    // map detail
    var mapLat = Model.GoogleMapLatitude;
    var mapLong = Model.GoogleMapLongitude;
    var mapAddress = Model.GoogleMapMarkerAddress;

