<style type="text/css">
    @@media (max-width: 767px) {
        .filter-box {
            padding: 0px;
        }
    }

    .filter-box {
        width: 95% !important;
    }

    .inner-section {
        padding: 0px;
    }
</style>
<!--Start Main Banner-->
@{
    var contactBannerImg = Model.ContactMainBanner;
    if (contactBannerImg != null)
    {
        <section class="main-banner">
            <div class="container-fluid custom-container-fluid">
                <div class="main-slide">
                    <div class="slide">
                        <img src="@contactBannerImg.Url" alt="@Model.ContactMainTitle" class="img-fluid" />
                    </div>
                </div>
            </div>
        </section>
    }
}
<!--End Main Banner-->
<!--Start Inner Page Wrapper-->
<div class="inner-section">
    <div class="container-fluid custom-container-fluid">
        <div class="row">
            <div class="col-md-12">
                <!--Start Breadcrumbs-->
                <nav aria-label="breadcrumb" class="breadcrumb-holder">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="@Umbraco.GetDictionaryValue("HomeHref")">@Umbraco.GetDictionaryValue("Home")</a></li>
                        <li class="breadcrumb-seprator"><i class="fas fa-angle-right"></i></li>
                        <li class="breadcrumb-item active" aria-current="page">@Model.ContactMainTitle</li>
                    </ol>
                </nav>
                <!--End Breadcrumbs-->
                <div class="filter-box page-content">
                    <div class="row">
                        <div class="col-md-5 col-sm-12 col-xs-12">
                            <!-- .contact-cont-left -->
                            @Html.Raw(Model.LocateAlJimiIntro)
                            <!-- .contact-cont-left -->

                            <div class="contact-cont-right">
                                <div class="contact-cont-wrapper">
                                    <h5>@Model.GeneralOpeningHoursTitle</h5>
                                    <ul class="oh-stores">
                                        @getOpeningHours("generalHoursLine1Days", "generalHoursLine1Hours")
                                        @getOpeningHours("generalHoursLine2Days", "generalHoursLine2Hours")
                                        @getOpeningHours("generalHoursLine3Days", "generalHoursLine3Hours")
                                    </ul>
                                    <h5>@Model.SpecialtyOpeningHoursTitle</h5>
                                    <ul class="oh-stores">
                                        @getOpeningHours("specialtyHoursLine1Store", "specialtyHoursLine1Hours")
                                        @getOpeningHours("specialtyHoursLine2Store", "specialtyHoursLine2Hours")
                                        @getOpeningHours("specialtyHoursLine3Store", "specialtyHoursLine3Hours")
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-7 col-sm-12 col-xs-12 contact-formDiv">
                            <div class="contact-form" id="form">
                                <div class="container">
                                    <div class="row">
                                        <div class="col-md-12 col-md-offset-1">
                                            @Html.Raw(Model.EnquiryFormTitle)
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form">
                                                @Umbraco.RenderMacro("renderUmbracoForm", new { FormGuid = Model.ContactForm.ToString(), FormTheme = "contactus-formtheme", ExcludeScripts = "1" })
                                                <div class="field-validation-error" id="divErrorXSS" style="display: none;color:red;">Please a provide valid value</div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- .contact-cont-right -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--End Inner Page Wrapper-->
<div class="page-content">
    <div id="map-location">
        <iframe id="map_canvas" src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d14523.653789645658!2d54.6087162!3d24.4884556!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0xe989e0e2486d0718!2sYas%20Mall!5e0!3m2!1sen!2s!4v1612183871486!5m2!1sen!2s"
                width="100%" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
        <div class="location-details">
            <h3>@Model.DirectionTitle</h3>
            <p>@Html.Raw(Model.DirectionBigDetails)</p>
            @{
                var mapPDFImg = Model.Value<IPublishedContent>("downloadPDFMap");
                if (mapPDFImg != null)
                {
                    <div class="buttons">
                        <a class="btn btn-primary gtm-track-button-event" target="_blank" href="@mapPDFImg.Url">@Umbraco.GetDictionaryValue("DownloadPDFMap")</a>
                    </div>
                }
            }
        </div>
        <!--Google Map Start-->
        <div id="map_canvas"></div>
        <!--Google Map End-->
    </div>
</div>
