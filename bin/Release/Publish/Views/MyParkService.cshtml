@using ContentModels = Umbraco.Web.PublishedModels;
@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.WeShopForYouDoc>
@using ClientDependency.Core.Mvc
@{
    var lang = System.Globalization.CultureInfo.CurrentCulture.TwoLetterISOLanguageName;
    Layout = "master.cshtml";

    // set page title segment
    ViewBag.PageTitle = Model.WsfyTitle;

    // meta fallback
    ViewBag.MetaDescription = Model.WsfySmallDetails;

    Html.RequiresCss("~/css/video-js.css");
}

<style type="text/css">
    .videos-tab .tab-navigation .nav-link .tabvdo-holder {
        background-color: transparent;
    }

    .aboutinfo-box .aboutinfo-image-holder .aboutinfo-image figure:before {
        background-color: transparent;
    }
</style>
<!--Start Main Banner-->
<section class="main-banner">
    <div class="container-fluid custom-container-fluid">
        <div class="main-slide">
            @{
                var myparkBannerImg = Model.Value<IPublishedContent>("wsfyBanner");
                if (myparkBannerImg != null)
                {
                    <div class="slide">
                        <img src="@myparkBannerImg.Url()" alt="@Model.Value("wsfyTitle")" class="img-fluid" />
                    </div>
                }
            }
        </div>
    </div>
</section>
<!--End Main Banner-->
<!--Start Inner Page Wrapper-->
<div class="inner-section">
    <div class="container-fluid custom-container-fluid">
        <div class="row">
            <div class="col-md-12">
                <!--Start Breadcrumbs-->
                <nav aria-label="breadcrumb" class="breadcrumb-holder">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="@Umbraco.GetDictionaryValue("LogoHref")">@Umbraco.GetDictionaryValue("Home")</a></li>
                        <li class="breadcrumb-seprator"><i class="fas fa-angle-right"></i></li>
                        <li class="breadcrumb-item active" aria-current="page">@Model.Value("wsfyTitle")</li>
                    </ol>
                </nav>
                <!--End Breadcrumbs-->
                <!--Start Main Title-->
                <div class="clearfix title-holder">
                    <h1 class="title">@Model.Value("wsfyTitle")</h1>
                </div>
                <!--End Main Title-->
                <!--Start Akyasi Tabs-->
                <section class="akyasi-holder">
                    <!--Start Tabs Holder-->
                    <div class="akyasi-tabs-holder custom-tabs">
                        <!--Start Custom Tabs Title-->
                        <ul class="nav nav-tabs" id="wsfyTab" role="tablist">
                            <li class="nav-item" role="presentation">
                                <a class="nav-link active" id="about-tab">@(lang == "ar" ? "عن أكياسي" : "About My Park")</a>
                            </li>
                            <li class="nav-item" role="presentation">
                                <a class="nav-link" id="tnc-tab">@(lang == "ar" ? "الشروط والأحكام" : "How To Use?")</a>
                            </li>
                            <li class="nav-item" role="presentation">
                                <a class="nav-link" id="faq-tab">@(lang == "ar" ? "الأسئلة الشائعة عن أكياسي" : "My Park FAQs")</a>
                            </li>
                        </ul>
                        <!--End Custom Tabs Title-->
                        <div class="tab-content" id="wsfyTabContent">
                            <!--Start About MyPark-->
                            <div class="tab-pane fade show active" id="about" role="tabpanel" aria-labelledby="about-tab">
                                <div class="clearfix abouttab-holder">
                                    <div class="content-box">
                                        @Html.Raw(Model.Value("wsfySmallDetails"))
                                    </div>
                                    <div class="clearfix videobox-holder">
                                        <div class="row videos-tab">
                                            <div class="col-lg-4 col-md-6 order-md-12 tab-navigation">
                                                <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                                                    <a class="nav-link active" id="video-tab-01" href="#video-01"
                                                       role="tab" aria-controls="video-01" aria-selected="true">
                                                        <figure class="tabvdo-img">
                                                            @{
                                                                var wsfyVideoFBImg1 = Model.Value<IPublishedContent>("wsfyVideo1FallbackImg");
                                                                if (wsfyVideoFBImg1 != null)
                                                                {
                                                                    <img src="@wsfyVideoFBImg1.Url()" alt="@Model.Value("wsfyTitle")" class="img-fluid" />
                                                                }
                                                            }
                                                        </figure>
                                                        <div class="tabvdo-holder">
                                                            <div class="tabvdo-content">
                                                                @Html.Raw(Model.Value("wsfyVideo1Info"))
                                                                <span class="tabvdo-play">
                                                                    <i class="fas fa-play-circle"></i>
                                                                    <strong>@(lang == "ar" ? "شاهدوا الآن!" : "Watch Now!")</strong>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </a>
                                                    <a class="nav-link" id="video-tab-02" href="#video-02"
                                                       role="tab" aria-controls="video-02" aria-selected="false">
                                                        <figure class="tabvdo-img">
                                                            @{
                                                                var wsfyVideoFBImg2 = Model.Value<IPublishedContent>("wsfyVideo2FallbackImg");
                                                                if (wsfyVideoFBImg2 != null)
                                                                {
                                                                    <img src="@wsfyVideoFBImg2.Url()" alt="@Model.Value("wsfyTitle")" class="img-fluid" />
                                                                }
                                                            }
                                                        </figure>
                                                        <div class="tabvdo-holder">
                                                            <div class="tabvdo-content">
                                                                @Html.Raw(Model.Value("wsfyVideo2Info"))
                                                                <span class="tabvdo-play">
                                                                    <i class="fas fa-play-circle"></i>
                                                                    <strong>@(lang == "ar" ? "شاهدوا الآن!" : "Watch Now!")</strong>
                                                                </span>
                                                            </div>
                                                        </div>
                                                    </a>
                                                </div>
                                            </div>
                                            <div class="col-lg-8 col-md-6 order-md-1 tab-player gtm-mainsec" data-section="mypark-video">
                                                <div class="tab-content" id="v-pills-tabContent">
                                                    <div class="tab-pane fade show active" id="video-01" role="tabpanel" aria-labelledby="video-tab-01">
                                                        @{
                                                            var wsfyVideoPoster = Model.Value<IPublishedContent>("wsfyVideoPoster");
                                                            var wsfyVideo1 = Model.Value<IPublishedContent>("wsfyVideo1");
                                                            if (wsfyVideoPoster != null && wsfyVideo1 != null)
                                                            {
                                                                <video id="myvideo-1" class="video-js" name="My Park Shopping Mall-VideoPlay" controls preload="auto" width="890" height="500"
                                                                       poster="@wsfyVideoPoster.Url()" data-setup="{}">
                                                                    <source src="@wsfyVideo1.Url()" type="video/mp4" />
                                                                    <p class="vjs-no-js">
                                                                        To view this video please enable JavaScript, and consider upgrading to a web browser that
                                                                        <a href="https://videojs.com/html5-video-support/" target="_blank">supports HTML5 video</a>
                                                                    </p>
                                                                </video>
                                                            }
                                                        }
                                                    </div>
                                                    <div class="tab-pane fade" id="video-02" role="tabpanel" aria-labelledby="video-tab-02">
                                                        @{
                                                            var wsfyVideo2 = Model.Value<IPublishedContent>("wsfyVideo2");
                                                            if (wsfyVideoPoster != null && wsfyVideo2 != null)
                                                            {
                                                                <video id="myvideo-2" class="video-js" name="My Park App Download-VideoPlay" controls preload="auto" width="890" height="500"
                                                                       poster="" data-setup="{}">
                                                                    <source src="@wsfyVideo2.Url()" type="video/mp4" />
                                                                    <p class="vjs-no-js">
                                                                        To view this video please enable JavaScript, and consider upgrading to a web browser that
                                                                        <a href="https://videojs.com/html5-video-support/" target="_blank">supports HTML5 video</a>
                                                                    </p>
                                                                </video>
                                                            }
                                                        }
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="clearfix aboutinfo-holder">
                                        <div class="row">
                                            <div class="col-lg-8 col-md-12 aboutinfo-listing">
                                                <div class="aboutinfo-box">
                                                    <div class="row">
                                                        <div class="col-md-6 aboutinfo-content-holder">
                                                            <div class="aboutinfo-content">
                                                                <div class="aboutinfo-bottom">
                                                                    @if (lang == "ar")
                                                                    {
                                                                        <h5>
                                                                            اركن الآن في موقفك المفضل في ياس مول عندما تشاء
                                                                        </h5>
                                                                        <p>تعرف على كيفية استخدام تطبيق موقفي في ياس مول</p>
                                                                        <p><a class="scond-btn" id="faq-tab-btn" href="#faq">قراءة الأسئلة الشائعة</a></p>
                                                                    }
                                                                    @if (lang == "en")
                                                                    {
                                                                        <h5>Now park in your most preferred parking spot at YAS Mall whenever you want</h5>
                                                                        <p>Know how to use My Park at YAS Mall</p>
                                                                        <p><a class="scond-btn" id="faq-tab-btn" href="#faq">Read FAQs</a></p>
                                                                    }
                                                                    <figure>
                                                                        <img src="/assets/images/icon-parking.png" alt="" class="img-fluid" />
                                                                    </figure>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 aboutinfo-image-holder">
                                                            <div class="aboutinfo-image">
                                                                <figure>
                                                                    @{
                                                                        var wsfyMessageUsImg = Model.Value<IPublishedContent>("wsfyMessageUsBack");
                                                                        if (wsfyMessageUsImg != null)
                                                                        {
                                                                            <img src="@wsfyMessageUsImg.Url()" alt="@Model.Value("wsfyTitle")" class="img-fluid" />
                                                                        }
                                                                    }
                                                                </figure>
                                                                <div class="aboutinfo-image-content">
                                                                    @if (lang == "ar")
                                                                    {
                                                                        <h5>سهل الاستخدام!</h5>
                                                                        <p>.حمل تطبيق موقفي وسجل بسرعة باستخدام فيسبوك أو بريدك الإلكتروني</p>
                                                                    }
                                                                    @if (lang == "en")
                                                                    {
                                                                        <h5>So easy to use!</h5>
                                                                        <p>Download the MyPark app & quickly register using Facebook or with your Email address.</p>
                                                                    }
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="aboutinfo-box">
                                                    <div class="row">
                                                        <div class="col-md-6 aboutinfo-image-holder">
                                                            <div class="aboutinfo-image">
                                                                <figure>
                                                                    @{
                                                                        var wsfyEasyPayImg = Model.Value<IPublishedContent>("wsfyEasyPayBack");
                                                                        if (wsfyEasyPayImg != null)
                                                                        {
                                                                            <img src="@wsfyEasyPayImg.Url()" alt="@Model.Value("wsfyTitle")" class="img-fluid" />
                                                                        }
                                                                    }
                                                                </figure>
                                                                <div class="aboutinfo-image-content">
                                                                    @if (lang == "ar")
                                                                    {
                                                                        <h5>لا مزيد من إحباط البحث عن مواقف والانتظار بعد اليوم!</h5>
                                                                        <p>.مع موقفي، يمكنك ركن سيارتك في أفضل موقف متوفر ومتميز ، احتفظ بمفاتيحك، ابقى قدر ما تشاء وغادر فوراً وقتما تشاء</p>
                                                                    }
                                                                    @if (lang == "en")
                                                                    {
                                                                        <h5>No more parking or waiting frustration!</h5>
                                                                        <p>With MyPark, you park in the best available premium spot, keep your keys, stay as long as you want and leave immediately when you want.</p>
                                                                    }
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="col-md-6 aboutinfo-content-holder">
                                                            <div class="aboutinfo-content">
                                                                <div class="aboutinfo-bottom">
                                                                    @if (lang == "ar")
                                                                    {
                                                                        <h5>انه بسيط جداً!</h5>
                                                                        <p>.موقفي يسمح لك بالعثور، الحجز والوصول إلى مكان موقفك في ياس مول، كل هذا من خلال هاتفك الذكي، دون عناء!</p>
                                                                        <p><a class="scond-btn" id="tnc-tab-btn" href="#tnc">اقرأ الأسئلة المتداولة</a></p>
                                                                    }
                                                                    @if (lang == "en")
                                                                    {
                                                                        <h5>It’s so simple!</h5>
                                                                        <p>MyPark allows you to find, reserve, and access your parking spot at YAS Mall, from your smartphone, hassle free!</p>
                                                                        <p><a class="scond-btn" id="tnc-tab-btn" href="#tnc">How To Use?</a></p>
                                                                    }
                                                                    <figure>
                                                                        <img src="/assets/images/icon-tap.png" alt="" class="img-fluid" />
                                                                    </figure>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-4 col-md-12 aboutapp-info-holder">
                                                <div class="aboutapp-info">
                                                    <figure>
                                                        @{
                                                            var wsfyDownloadAppImg = Model.Value<IPublishedContent>("wsfyDownloadAppBack");
                                                            if (wsfyDownloadAppImg != null)
                                                            {
                                                                <img src="@wsfyDownloadAppImg.Url()" alt="@Model.Value("wsfyTitle")" class="img-fluid" />
                                                            }
                                                        }
                                                    </figure>
                                                    <div class="aboutapp-content">
                                                        <figure class="logo-image">
                                                            <img src="/assets/images/mypark-logo.png" alt="" class="img-fluid" />
                                                        </figure>
                                                        @if (lang == "ar")
                                                        {
                                                            <p class="lg-text">حمل تطبيق موقفي الآن!</p>
                                                            <p class="lg-text">اختار متجرك للتطبيقات وحمل هذا التطبيق المجاني الآن!</p>
                                                        }
                                                        @if (lang == "en")
                                                        {
                                                            <p class="lg-text">Download MyPark App Now!</p>
                                                            <p class="lg-text">Choose your App Store and download the free application now!</p>
                                                        }
                                                        <figure class="app-icons">
                                                            <a href="https://bit.ly/2O6iXkb" class="gtm-track-app-event" name="Google Play">
                                                                <img src="/assets/images/icon-googleplay.png" alt="" class="img-fluid" />
                                                            </a>
                                                            <a href="https://apple.co/2ZNCfQG" class="gtm-track-app-event" name="App Store">
                                                                <img src="/assets/images/icon-appstore.png" alt="" class="img-fluid" />
                                                            </a>
                                                        </figure>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--End About MyPark-->
                            <!--Start How To Use-->
                            <div class="tab-pane fade" id="tnc" role="tabpanel" aria-labelledby="tnc-tab">
                                <div class="row">
                                    <div class="col-md-2 col-sm-12 col-xs-12">
                                    </div>
                                    <div class="col-md-8 col-sm-12 col-xs-12">
                                        @if (lang == "ar")
                                        {
                                            <p>لاستخدام MyPark في Yas Mall اتبع الخطوات التالية:</p>
                                            <ul class="seprator-list">
                                                <li>تنزيل تطبيق MyPark على iPhone أو Android</li>
                                                <li>تسجيل حسابك باستخدام رقم بريدك الإلكتروني/هاتفك</li>
                                                <li>احجز مساحة ركن السيارة باستخدام التطبيق</li>
                                                <li>تحديد موقع قسم الركن أ أو اتبع الإشارات على MyPark في موقف السيارات</li>
                                                <li>سوف يكون جهاز يحمل المساحة الخاصة بك في انتظارك</li>
                                                <li>استخدم هاتفك الذكي لتأكيد وصولك</li>
                                                <li>أوقف سيارتك!</li>
                                            </ul>
                                            <br />
                                            <p>
                                                يرجى الملاحظة بأنه في حال عدم توفر أماكن لوقوف السيارات، لن تتمكن من متابعة الخطوات أعلاه.
                                            </p>
                                        }
                                        @if (lang == "en")
                                        {
                                            <p>To use MyPark at Yas Mall follow the steps below:</p>
                                            <ul class="seprator-list">
                                                <li>Download the MyPark App on iPhone or Android</li>
                                                <li>Register your account using your email/phone number</li>
                                                <li>Book your parking space using the App</li>
                                                <li>Locate parking section A or follow the MyPark signs in the parking lot</li>
                                                <li>A device holding your space will be waiting for you</li>
                                                <li>Use your smartphone to confirm your arrival</li>
                                                <li>Park your car!</li>
                                            </ul>
                                            <br />
                                            <p>
                                                Please note if no parking spaces are available you will not be able to proceed with the steps above.
                                            </p>
                                        }
                                    </div>
                                    <div class="col-md-2 col-sm-12 col-xs-12">
                                    </div>
                                </div>
                            </div>
                            <!--End How To Use-->
                            <!--Start FAQs-->
                            <div class="tab-pane fade" id="faq" role="tabpanel" aria-labelledby="faq-tab">
                                <div class="accordion custom-accordion" id="accordionFAQ">
                                    @if (lang == "ar")
                                    {
                                        <div class="row">
                                            <div class="col-lg-6">
                                                <div class="card">
                                                    <div class="card-header" id="heading1">
                                                        <h2 class="mb-0">
                                                            <button class="btn btn-link btn-block" type="button">
                                                                ماهو  MyPark؟
                                                                <span class="btn-collapse">
                                                                    <i class="fas fa-plus"></i>
                                                                    <i class="fas fa-minus"></i>
                                                                </span>
                                                            </button>
                                                        </h2>
                                                    </div>
                                                    <div id="collapse1" class="collapse show" aria-labelledby="heading1">
                                                        <div class="card-body">
                                                            MyPark  هو نظام حجز ثوري يسمح لك بحجز مكان ممتاز، 15 دقيقة أو حتى ستة أشهر قبل الموعد، فقط باستخدام تطبيق على هاتفك.
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="card">
                                                    <div class="card-header" id="heading2">
                                                        <h2 class="mb-0">
                                                            <button class="btn btn-link btn-block collapsed" type="button">
                                                                لماذا تختلف عن الخادم؟
                                                                <span class="btn-collapse">
                                                                    <i class="fas fa-plus"></i>
                                                                    <i class="fas fa-minus"></i>
                                                                </span>
                                                            </button>
                                                        </h2>
                                                    </div>
                                                    <div id="collapse2" class="collapse" aria-labelledby="heading2">
                                                        <div class="card-body">
                                                            مع MyPark، يمكنك ركن نفسك في أفضل مساحة ركن سيارات ممتازة متوفرة والحصول على المفاتيح التي تريد الاحتفاظ بها.
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="card">
                                                    <div class="card-header" id="heading3">
                                                        <h2 class="mb-0">
                                                            <button class="btn btn-link btn-block collapsed" type="button">
                                                                كم من الوقت يمكنني الركن في مكان ما؟
                                                                <span class="btn-collapse">
                                                                    <i class="fas fa-plus"></i>
                                                                    <i class="fas fa-minus"></i>
                                                                </span>
                                                            </button>
                                                        </h2>
                                                    </div>
                                                    <div id="collapse3" class="collapse" aria-labelledby="heading3">
                                                        <div class="card-body">
                                                            طالما أنك تريد ذلك. سيتم فرض رسوم عليك بعد مغادرتك، استنادًا إلى الأسعار في Yas Mall.
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="card">
                                                    <div class="card-header" id="heading4">
                                                        <h2 class="mb-0">
                                                            <button class="btn btn-link btn-block collapsed" type="button">
                                                                هل هناك رسوم لاستخدام MyPark؟
                                                                <span class="btn-collapse">
                                                                    <i class="fas fa-plus"></i>
                                                                    <i class="fas fa-minus"></i>
                                                                </span>
                                                            </button>
                                                        </h2>
                                                    </div>
                                                    <div id="collapse4" class="collapse" aria-labelledby="heading4">
                                                        <div class="card-body">
                                                            نعم. يمكنك استخدام رمز العرض الترويجي "MYPARK1" مجانًا للمرة الأولى.
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="card">
                                                    <div class="card-header" id="heading5">
                                                        <h2 class="mb-0">
                                                            <button class="btn btn-link btn-block collapsed" type="button">
                                                                هل يعرض التطبيق كل المواقف المتوفرة في المنطقة؟
                                                                <span class="btn-collapse">
                                                                    <i class="fas fa-plus"></i>
                                                                    <i class="fas fa-minus"></i>
                                                                </span>
                                                            </button>
                                                        </h2>
                                                    </div>
                                                    <div id="collapse5" class="collapse" aria-labelledby="heading5">
                                                        <div class="card-body">
                                                            نعم، سيوضح الأقرب أولاً.
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="card">
                                                    <div class="card-header" id="heading12">
                                                        <h2 class="mb-0">
                                                            <button class="btn btn-link btn-block collapsed" type="button">
                                                                كيف يمكنني الحصول على الاتجاهات للوصول إلى موقف السيارات؟
                                                                <span class="btn-collapse">
                                                                    <i class="fas fa-plus"></i>
                                                                    <i class="fas fa-minus"></i>
                                                                </span>
                                                            </button>
                                                        </h2>
                                                    </div>
                                                    <div id="collapse12" class="collapse" aria-labelledby="heading12">
                                                        <div class="card-body">
                                                            عند تحديد الموقع، اضغط على زر "الاتجاهات" الأصفر الموجود على يسار الصورة لأخذك إلى الخريطة المفضلة في التطبيق بمجرد وصولك إلى الموقع، حدد الزر "رسم تخطيطي للقسم". يتم تحديد كل موقع بوضوح بواسطة إشارات لتسهيل العثور على المساحة التي تم تعيينها لك. يتم تحديد كل رقم مسافة بوضوح على الحائط أو على منشور لافتة.
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="card">
                                                    <div class="card-header" id="heading6">
                                                        <h2 class="mb-0">
                                                            <button class="btn btn-link btn-block collapsed" type="button">
                                                                لن أجري حجزي في الوقت المناسب، هل يمكنني إضافة المزيد من الوقت؟
                                                                <span class="btn-collapse">
                                                                    <i class="fas fa-plus"></i>
                                                                    <i class="fas fa-minus"></i>
                                                                </span>
                                                            </button>
                                                        </h2>
                                                    </div>
